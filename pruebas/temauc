## viendo lo de show_posets
b1,b2=Lat.find_models(4)

b1.uc=meet2uc(b1.operations["^"])
b2.uc=meet2uc(b2.operations["^"])
# tener uc no implica tener leq...
b1.relations["<="] = b1.get_leq()
b2.relations["<="] = b2.get_leq()

show_posets([b1,b2])

